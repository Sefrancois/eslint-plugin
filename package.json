{
  "name": "@sefr/eslint-plugin",
  "version": "1.3.13",
  "description": "ESLint plugin to enforce rules about software design like Onion architecture, modularity ...",
  "main": "./dist/index.js",
  "repository": {
    "url": "https://github.com/Sefrancois/eslint-plugin"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --no-splitting --minify",
    "format": "prettier --write src/ test/",
    "prepublish": "npm run build && npm test",
    "test": "vitest run"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "plugin-eslint"
  ],
  "author": "SEFR",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "~9.6.1",
    "@types/estree": "~1.0.6",
    "eslint": "~9.12.0",
    "prettier": "~3.3.3",
    "ts-node": "~10.9.2",
    "tsup": "~8.3.0",
    "typescript": "~5.6.2",
    "vitest": "~2.1.2",
    "vitest-mock-extended": "~2.0.2"
  }
}
