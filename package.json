{
  "name": "@sefr/eslint-plugin",
  "version": "1.3.12",
  "description": "ESLint plugin to enforce rules about software design like Onion architecture, modularity ...",
  "main": "./dist/index.js",
  "repository": {
    "url": "https://github.com/Sefrancois/eslint-plugin"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --no-splitting --minify",
    "format": "prettier --write src/ test/",
    "prepublish": "npm run build && npm test",
    "test": "vitest run"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "plugin-eslint"
  ],
  "author": "SEFR",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "~8.44.6",
    "@types/estree": "~1.0.3",
    "eslint": "~8.52.0",
    "prettier": "~3.0.3",
    "ts-node": "~10.9.1",
    "tsup": "~7.2.0",
    "typescript": "~5.2.2",
    "vitest": "~0.34.6",
    "vitest-mock-extended": "~1.3.1"
  }
}
